  <mapper namespace="com.lib.fin.member.DeptDAO">
  
  	<!-- file add -->
  	<select  id="getDeptInfo" resultType="Map">
  		
  		select *
  		from ST_STD_CD
  		where grp_cd = 'D001'
  		order by cd asc
  		
